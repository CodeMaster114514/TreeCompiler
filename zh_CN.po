#: main.c:18
msgid "Compile failed or compile error\n"
msgstr "编译失败或编译错误\n"

#: main.c:22
msgid "Everything compile fine\n"
msgstr "编译完成\n"

#: main.c:26
msgid "Unknow error occurred\n"
msgstr "发生未知错误\n"

#: compiler.c:23
msgid "on line %i,col %i in file %s\n"
msgstr "在第%i行，第%i位\n"

#: symresolver.c:114
msgid "can’t build variable symble\n"
msgstr "无法生成变量名符号\n"

#: symresolver.c:119
msgid "can’t build function symble\n"
msgstr "无法生成函数名符号\n"

#: lexer.c:54
msgid "E escape character compilation error\n"
msgstr "转义字符编译错误\n"

#: lexer.c:228
msgid "There is no corresponding symbol\n"
msgstr "这里不能换行\n"

#: lexer.c:368
#, c-format
msgid "The operator %s isn't valid\n"
msgstr "此操作符无效\n"

#: lexer.c:405
msgid "You can't close an expression that doesn' exist!\n"
msgstr "你不能结束一个未创造的括号\n"

#: lexer.c:609
msgid "you didn't close the mutiline_comment\n"
msgstr "你没有结束这段注释\n"

#: lexer.c:673
msgid "You didn't close the quote!\n"
msgstr "你漏了个'在这\n"

#: lexer.c:787
msgid "Unexpected token\n"
msgstr "不存在的令牌\n"

#: parser.c:161
#, c-format
msgid "Expect the symbol \"%c\" however, other symbols are provided.\n"
msgstr "你使用了错误的符号在这，你要使用“%c”\n"

#: parser.c:170
#, c-format
msgid "Expect the symbole \"%s\" however, other operator are provided.\n"
msgstr "你使用错误的符号，你要用操作符“%s”\n"

#: parser.c:179
#, c-format
msgid "Expect the keyword \"%s\" however, other keyword are provided.\n"
msgstr "你使用了错误的关键词\n"

#: parser.c:219
msgid "This isn't a single token that can be converted to a node.\n"
msgstr "这不是一个能直接转换成节点的令牌\n"

#: parser.c:320
msgid "We can't get number's point!\n"
msgstr "我们无法给数字取地址\n"

#: parser.c:323
msgid "Keyword should be use d in right place!\n"
msgstr "关键词需要用在正确的地方\n"

#: parser.c:326
msgid "Have something wrong in here.\n"
msgstr "看来这里出了些问题\n"

#: parser.c:425
msgid ""
"The operatoring object of the self-increment and subtraction operator must "
"be a variable\n"
msgstr "自增自减运算符的操作对象必须是变量\n"

#: parser.c:440
msgid "The condition must be a expressionable\n"
msgstr "判断式必须有值\n"

#: parser.c:624
msgid "There isn’t have such combination\n"
msgstr "没有这样类型组合\n"

#: parser.c:633
msgid "Doesn’t have data type \"int int\"\n"
msgstr "没有“int int”类型\n"

#: parser.c:659 parser.c:758
msgid "this data type can’t have secondary type\n"
msgstr "这里类型没有子类型\n"

#: parser.c:698
msgid "BUG: Other keywords have entered this function\n"
msgstr "漏洞：有不该出现的关键词在这\n"

#: parser.c:953
msgid "The size of variable can't be zero\n"
msgstr "变量大小不能为0\n"

#: parser.c:1407
msgid "BUG: Compiler failed creat data type\n"
msgstr "漏洞：创建数据类型失败\n"

#: parser.c:1426
#, c-format
msgid "\"%s %s\" has been initiallized\n"
msgstr "此类型已被声明\n"

#: parser.c:1436
msgid "Variable or function name must be identifier\n"
msgstr "函数名或变量名必须是标识符\n"

#: parser.c:1459
msgid "the variable name must is identifier\n"
msgstr "变量名必须是标识符\n"

#: parser.c:1688
msgid "The goto statement must use identifier, but you provide others.\n"
msgstr "goto语句必须要用表示符，但是你提供了别的东西\n"

#: parser.c:1707
msgid "We only spurre in case"
msgstr "这里必须用表达式"

#: parser.c:1857
#, c-format
msgid "We can't computed \"%lld %s %lld\"\n"
msgstr "我们无法计算“%lld %s %lld”\n"

#: parser.c:1910
msgid "The identifier must be used to declare the label\n"
msgstr "声明标签必须用标识符\n"
